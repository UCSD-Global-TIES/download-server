    "html-metadata": "^1.7.1",
    "link-check": "^4.4.7",
    "moment": "^2.24.0",
    "network-speed": "^2.0.5",
    "node-wifi": "^2.0.12",
    "react-router-dom": "^5.1.2",
    "request": "^2.88.2",
    "socket.io": "^2.3.0",
